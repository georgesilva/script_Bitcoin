$.cookie.raw = true;
if ($("#free_play_sound").is( ":checked" )){
	$.cookie('free_play_sound', 1, { expires: 3650 });
	free_play_sound = true;
}else{
	$.cookie('free_play_sound', 0, { expires: 3650 });
	free_play_sound = false;
