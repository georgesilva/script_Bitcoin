			$('#time_remaining').countdown({until: +result[1], format: 'MS'});
			setTimeout(function(){ RefreshPageAfterFreePlayTimerEnds(); }, parseInt(result[1])*1000);
			title_countdown (parseInt(result[1]));$('#free_play_digits').hide();
			clearInterval(intervalID);
		}if (result[0] == "e4"){
			$('.free_play_element').show();
			$('#free_play_error').show();
			$('#free_play_error').html("Session Expired. <a href='https://freedoge.co.in/?op=home'>Please click here to reload the page</a>.");
			$('#free_play_digits').hide();clearInterval(intervalID);
		}if (result[0] == "e8"){
			$('.free_play_element').show();
			$('#free_play_error').show();
			$('#verify_mobile_message').show();
			$('#free_play_error').html("Sorry, this IP address has been blocked. If you are using a proxy or anonymization service, please turn it off before playing.");
			$('#free_play_digits').hide();
			ACPuzzle.reload();
			grecaptcha.reset();
			Recaptcha.reload();
			clearInterval(intervalID);
		}if (result[0] == "e9"){
			$('.free_play_element').show();
			$('#free_play_error').show();
			$('#free_play_error').html("Client Seed cannot be empty.<BR>Please enter one by clicking on the PROVABLY FAIR link above.");
			$('#free_play_digits').hide();
			ACPuzzle.reload();
			grecaptcha.reset();
			Recaptcha.reload();
			clearInterval(intervalID);
		}if (result[0] == "e10"){
			$('.free_play_element').show();
			$('#free_play_error').show();
			$('#verify_mobile_message_email').show();
			$('#free_play_error').html("You have an invalid email address attached to your account.<BR>You need to change it to a valid one by clicking on PROFILE before you can play.");
			$('#free_play_digits').hide();
			ACPuzzle.reload();
			grecaptcha.reset();
			Recaptcha.reload();
			clearInterval(intervalID);
		}if (result[0] == "e11"){
			$('.free_play_element').show();
			$('#free_play_error').show();
			$('#free_play_error').html("This website is completely supported by revenue from ads, which is used to buy dogecoins to distribute to users like you.<BR>By blocking ads on our website, you are cutting off our only source of revenue and this will seriously affect our ability to continue distributing dogecoins to our users.<BR>Please disable your ad-blocking browser plugin/software for this page and then <a href='http://freedoge.co.in/?op=home'>click here</a> to refresh the page and collect your free dogecoins");
			$('#free_play_digits').hide();
			ACPuzzle.reload();
			grecaptcha.reset();
			Recaptcha.reload();
		}if (result[0] == "s1"){
			$.cookie.raw = true;
			$.cookie('last_play', result[4], { expires: 3650 });
			$('.free_play_element').hide();
			$('#free_play_result').show();
			var number=result[1];
			var single_digit=number.split(""); 
			if (number.toString().length < 5){
				var remaining=5-number.toString().length;
				for (var i=0;i<remaining;i++){
					single_digit.unshift('0');
				}
			}
			clearInterval(intervalID);
			$('#free_play_digits').html(single_digit[0]+single_digit[1]+single_digit[2]+single_digit[3]+single_digit[4]);
			$('#wait').show();
			$('#balance').html(result[2]);
			$('#balance2').html(result[2]);
			$('#time_remaining').countdown({until: +3600, format: 'MS'});
			setTimeout(function(){ RefreshPageAfterFreePlayTimerEnds(); }, 3600*1000);
			title_countdown (3600);
			free_play_claim_amount = parseFloat(Math.round(result[3] * 100) / 100).toFixed(2);
			$('#winnings').html(free_play_claim_amount);
			$('#balance_usd').html(result[5]);
			$('#next_server_seed_hash').val(result[6]);
			$('#next_nonce').html(result[8]);
			$('.previous_server_seed').html(result[9]);
			$('#previous_server_seed_hash').val(result[10]);
			$('.previous_client_seed').html(result[11]);
			$('.previous_nonce').html(result[12]);
			$('#previous_roll').html(result[1]);
			$('#no_previous_rolls_msg').hide();
			$('#previous_rolls_table').show();
			$('#previous_roll_strings').show();
			$("#verify_rolls_link").attr("href", "https://s3.amazonaws.com/roll-verifier/verify.html?server_seed="+result[9]+"&client_seed="+result[11]+"&server_seed_hash="+result[10]+"&nonce="+result[12]);}$("#free_play_form_button").attr("disabled", false);
		});
		posting.fail(function() {$('.free_play_element').show();$('#free_play_error').show();$('#free_play_error').html("Request timed out. Please try again.");$('#free_play_digits').hide();clearInterval(intervalID);$("#free_play_form_button").attr("disabled", false);});}});$("#set_email_preferences").click(function() {$("#set_email_preferences").attr("disabled", true);var subs_arr = $('.email_subs_checkbox:checkbox:checked').map(function() { return this.value;}).get().toString();$.get('/?op=set_email_subscriptions&subs='+subs_arr, function(data) {$("#set_email_preferences").attr("disabled", false);$("#email_subs_success").show();});});





$("#double_your_doge_payout_multiplier").keyup(function() {
	if (parseFloat($("#double_your_doge_payout_multiplier").val()) < 1.01 && parseFloat($("#double_your_doge_payout_multiplier").val()) != 1){
		$("#double_your_doge_payout_multiplier").val(1.01);
	}else if (parseFloat($("#double_your_doge_payout_multiplier").val()) > 4750.00){
		$("#double_your_doge_payout_multiplier").val(4750.00);
	}
	CalculateWinAmount();
	$("#double_your_doge_win_chance").val((parseFloat(parseInt($( ".lt" ).html())/10000*100).toFixed(2))+"%");
});
$("#double_your_doge_win_chance").keyup(function(){
	if (parseFloat($("#double_your_doge_win_chance").val()) > 94.06){
		$("#double_your_doge_win_chance").val(94.06+"%");
	}else if (parseFloat(
		$("#double_your_doge_win_chance").val()) < 0.02 && parseFloat($("#double_your_doge_win_chance").val()) != 0){
		$("#double_your_doge_win_chance").val(0.02+"%");
	}
	$("#double_your_doge_payout_multiplier").val(parseFloat(95/parseFloat($("#double_your_doge_win_chance").val())).toFixed(2));
	CalculateWinAmount();
});
