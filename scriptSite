var stop_autobet=false;
var autobet_dnr=false;
var autobet_running=false;
var free_play_sound=false;
var autobet_history=[];

$.ajaxSetup({
	data:{
		csrf_token:$.cookie('csrf_token')
	}, beforeSend:function(xhr){
		xhr.setRequestHeader('x-csrf-token',$.cookie('csrf_token'));
	}, timeout:60000
});

$.extend({
	redirectPost:function(location,args){
		var form='';$.each(args,function(key,value){
			form+='<input type="hidden" name="'+key+'" value="'+value+'">';
		});
		$('<form action="'+location+'" method="POST">'+form+'</form>').appendTo('body').submit();
	}
});

$(document).ready(function(){
	$("#hide_site_message").click(function(){
		$('#common_site_message').hide();
		$.get('/?op=hide_site_message');
	});
	charSet='ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789';
	var randomString='';
	for(var i=0;i<16;i++){
		var randomPoz=Math.floor(Math.random()*charSet.length);
		randomString+=charSet.substring(randomPoz,randomPoz+1);
	}
	$('#next_client_seed').val(randomString);
	$('.tabs a').click(function(){
		$('.tabs li').removeClass('active');
		$(this).parent().addClass('active');
	});
	$('#free_play_link_li').addClass('active');
	$('.faq_answer').hide();
	$('#what_is_dogecoin_signup_page_more').hide();
	$('.faq_question').click(function(){
		$(this).next('.faq_answer').show();
	});
	$('#what_is_dogecoin_signup_page_read_more_link').click(function(){
		$('#what_is_dogecoin_signup_page_read_more_link').hide();
		$('#what_is_dogecoin_signup_page_more').show();
	});
	$('#provably_fair_link').click(function(){
		$("html, body").animate({scrollTop:$("#provably_fair").offset().top- 45},"fast");
	});
	$("#signup_form").submit(function(event){event.preventDefault();
	$("#signup_button").attr("disabled",true);
	var fingerprint=$.fingerprint();
	var tag=$.url().param("tag");
	var $form=$(this),
		op=$form.find('input[name="op"]').val(),
		referrer=$form.find('input[name="referrer"]').val(),
		doge_address=$form.find('input[name="doge_address"]').val(),
		password=$form.find('input[name="password"]').val(),
		email=$form.find('input[name="email"]').val(),
		g_recaptcha_response=$form.find('#g-recaptcha-response').val()||grecaptcha.getResponse(),
		adcopy_challenge=$form.find('input[name="adcopy_challenge"]').val(),
		adcopy_response=$form.find('input[name="adcopy_response"]').val()||$form.find('select[name="adcopy_response"]').val(),
		token=$form.find('input[name="token"]').val(),
		url=$form.attr('action');
	var posting=$.post(url,{
		op:op,
		doge_address:doge_address,
		password:password,
		email:email,
		adcopy_challenge:adcopy_challenge,
		adcopy_response:adcopy_response,
		fingerprint:fingerprint,
		referrer:referrer,
		tag:tag,
		token:token,
		g_recaptcha_response:g_recaptcha_response});
		posting.done(function(data){
			var result=data.split(":");
			$('#signup_error').hide();
			$('#signup_error').html("");
			$('#signup_form_doge_address').removeClass('input-error');
			$('#signup_form_password').removeClass('input-error');
			$('#signup_form_repeat_password').removeClass('input-error');
			if(result[0]=="e1"){$('#signup_error').show();
			$('#signup_error').html("Incorrect Dogecoin address entered");
			$('#signup_form_doge_address').addClass('input-error');
			ACPuzzle.reload();
			grecaptcha.reset();
		}
		if(result[0]=="e2"){
			$('#signup_error').show();
			$('#signup_error').html("Please enter a password");
			$('#signup_form_password').addClass('input-error');
			ACPuzzle.reload();
			grecaptcha.reset();
		}
		if(result[0]=="e3"){
			$('#signup_error').show();
			$('#signup_error').html("Password and repeat password do not match");
			$('#signup_form_password').addClass('input-error');
			$('#signup_form_repeat_password').addClass('input-error');
			ACPuzzle.reload();
			grecaptcha.reset();
		}
		if(result[0]=="e4"){
			$('#signup_error').show();
			$('#signup_error').html("An account with this Dogecoin address already exists");
			$('#signup_form_doge_address').addClass('input-error');
			ACPuzzle.reload();
			grecaptcha.reset();
		}
		if(result[0]=="e5"){
			$('#signup_error').show();
			$('#signup_error').html("Invalid email address");
			$('#signup_form_doge_address').addClass('input-error');
			ACPuzzle.reload();
			grecaptcha.reset();
		}if(result[0]=="e8"){
			$('#signup_error').show();
			$('#signup_error').html("Sorry, this IP address has been blocked. If you are using a proxy or anonymization service, please turn it off before playing. If not, you can get a new IP by restarting your router.");
		}
		if(result[0]=="e9"){
			$('#signup_error').show();
			$('#signup_error').html("Unexpected error. Reloading the page...");
			window.location.replace("http://freedoge.co.in/?op=home");
		}
		if(result[0]=="e10"){
			$('#signup_error').show();
			$('#signup_error').html("An account with this email address already exists");
			ACPuzzle.reload();
			grecaptcha.reset();
		}
		if(result[0]=="e11"){
			$('#signup_error').show();
			$('#signup_error').html("Please enter an email address");
			$('#signup_form_doge_address').addClass('input-error');
			ACPuzzle.reload();grecaptcha.reset();
		}
		if(result[0]=="e12"){
			$('#signup_error').show();
			$('#signup_error').html("Incorrect captcha entered");
			$('#recaptcha_captcha').show();
			ACPuzzle.reload();grecaptcha.reset();
		}
		if(result[0]=="s1"){
			$.cookie.raw=true;$.cookie('doge_address',result[1],{expires:3650});
			$.cookie('password',result[2],{expires:3650});
			$.cookie('have_account',1,{expires:3650});
			window.location.replace("http://freedoge.co.in/?op=home");
		}
		$("#signup_button").attr("disabled",false);
	});
});
$("#login_form").submit(function(event){
	event.preventDefault();
	$("#login_button").attr("disabled",true);
	var $form=$(this),
		op=$form.find('input[name="op"]').val(),
		doge_address=$form.find('input[name="doge_address"]').val(),
		password=$form.find('input[name="password"]').val(),
		recaptcha_challenge_field=$form.find('input[name="recaptcha_challenge_field"]').val(),
		recaptcha_response_field=$form.find('input[name="recaptcha_response_field"]').val(),
		adcopy_challenge=$form.find('input[name="adcopy_challenge"]').val(),
		adcopy_response=$form.find('input[name="adcopy_response"]').val()||$form.find('select[name="adcopy_response"]').val(),
		url=$form.attr('action');
	var posting=$.post(url,
	{
		op:op,
		doge_address:doge_address,
		password:password,
		recaptcha_challenge_field:recaptcha_challenge_field,
		recaptcha_response_field:recaptcha_response_field,
		adcopy_challenge:adcopy_challenge,
		adcopy_response:adcopy_response
	});
	posting.done(function(data){
		var result=data.split(":");
		$('#login_error').html("");
		$('#login_error').hide();
		if(result[0]=="e1"){
			$('#login_error').show();
			$('#login_error').html("Please enter a dogecoin address or email address");
		}if(result[0]=="e2"){
			$('#login_error').show();
			$('#login_error').html("Please enter a password");
		}if(result[0]=="e3"){
			$('#login_error').show();
			$('#login_error').html("Incorrect login details");
		}if(result[0]=="e4"){
			$('#login_error').show();
			$('#login_error').html("Incorrect captcha entered");
		}if(result[0]=="e5"){
			$('#login_error').show();
			$('#login_error').html("Incorrect login details. Reloading page..");
			window.location.replace("http://freedoge.co.in/?op=home");
		}if(result[0]=="e7"){
			$('#login_error').show();
			$('#login_error').html("Incorrect login details");
			}if(result[0]=="e6"){
				$('#login_error').show();
				$('#login_error').html("Sorry, this IP address has been blocked. If you are using a proxy or anonymization service, please turn it off before playing. If not, you can get a new IP by restarting your router.");
			}if(result[0]=="s1"){
				$.cookie.raw=true;$.cookie('doge_address',result[1],{expires:3650});
				$.cookie('password',result[2],{expires:3650});
				$.cookie('have_account',1,{expires:3650});
				window.location.replace("http://freedoge.co.in/?op=home");
			}
			$("#login_button").attr("disabled",false);
		});
});
