function BetErrors (code){if (code == "e1"){$('#double_your_doge_error').html("Insufficient balance to make this bet");
}if (code == "e2"){
	$('#double_your_doge_error').html("Bet amount cannot be less than "+min_win_amount+" DOGE");
}if (code == "e3"){
	$('#double_your_doge_error').html("Bet amount cannot be empty");
}if (code == "e4"){
	$('#double_your_doge_error').html("Invalid bet method");
}if (code == "e5"){
	$('#double_your_doge_error').html("Bet amount cannot be more than "+max_win_amount+" DOGE");
}if (code == "e6"){
	$('#double_your_doge_error').html("Unexpected error. Please log out and then log back in");
}if (code == "e7"){
	$('#double_your_doge_error').html("Payout multiplier has to be between 2x and 100x");
}if (code == "e8"){
	$('#double_your_doge_error').html("Win amount cannot be more than "+max_win_amount+" DOGE");
}if (code == "e9"){$('#double_your_doge_error').html("Your balance is insufficient to make this bet and try to win the jackpot<BR>Please un-select the jackpot bet option and try again");
}if (code == "e10"){
	$('#double_your_doge_error').html("Client Seed is either empty or has invalid characters (only letters and numbers allowed).<BR>Please correct it by clicking on the PROVABLY FAIR link above.");
}if (code == "e11"){
	$('#double_your_doge_error').html("Please wait for your previous bet to finish rolling.");
}if (code == "e12"){
	$('#double_your_doge_error').html("Betting is disabled in your country.");
}
}




$("#double_your_doge_2x").click(function(event){
	var bet=$("#double_your_doge_stake").val();
	var balance=$("#balance").html();
	if(bet*2<=balance){
		if(bet*2*($("#double_your_doge_payout_multiplier").val()- 1)<=max_win_amount){
			$("#double_your_doge_stake").val(parseFloat(Math.round(bet*2*100000000)/ 100000000).toFixed(8));
		}else{
			$("#double_your_doge_stake").val(parseFloat(Math.round(max_win_amount/($("#double_your_doge_payout_multiplier").val() - 1) * 100000000) / 100000000).toFixed(8));
		}
	}else{
		if ( bet*2*($("#double_your_doge_payout_multiplier").val() - 1) <= max_win_amount){
			$("#double_your_doge_stake").val(balance);
		}else{
			$("#double_your_doge_stake").val(parseFloat(Math.floor(max_win_amount/($("#double_your_doge_payout_multiplier").val() - 1) * 100000000) / 100000000).toFixed(8));
		}
	}
	CalculateWinAmount();
});
$("#double_your_doge_half").click(function(event) {
	var bet = $("#double_your_doge_stake").val();
	var balance = $("#balance").html();
	if (bet*0.5 >= min_win_amount){
		if (bet*0.5 <= balance){
			if ( bet*0.5*($( "#double_your_doge_payout_multiplier" ).val() - 1) <= max_win_amount){
				$("#double_your_doge_stake").val(parseFloat(Math.round(bet*0.5 * 100000000) / 100000000).toFixed(8));
			}else{
				$("#double_your_doge_stake").val(parseFloat(Math.round(max_win_amount/($("#double_your_doge_payout_multiplier").val() - 1) * 100000000) / 100000000).toFixed(8));
			}
		}else{
			if ( bet*0.5*($("#double_your_doge_payout_multiplier").val() - 1) <= max_win_amount){
				$("#double_your_doge_stake").val(balance);
			}else{
				$("#double_your_doge_stake").val(parseFloat(Math.round(max_win_amount/($("#double_your_doge_payout_multiplier").val() - 1) * 100000000) / 100000000).toFixed(8));
			}
		}
	}
	CalculateWinAmount();
});
$("#double_your_doge_max").click(function(event) {
	var bet = $("#double_your_doge_stake").val();
	var balance = $("#balance").html();
	if (balance*($( "#double_your_doge_payout_multiplier" ).val() - 1) <= max_win_amount){
		$("#double_your_doge_stake").val(balance);
	}else{
		$("#double_your_doge_stake").val(parseFloat(Math.round(max_win_amount/($("#double_your_doge_payout_multiplier").val() - 1) * 100000000) / 100000000).toFixed(8));
	}
	CalculateWinAmount();
});
$("#double_your_doge_min").click(function(event) {
	var balance = $("#balance").html();
	if (balance >= min_win_amount){
		$("#double_your_doge_stake").val(min_win_amount);
	}else{
		$("#double_your_doge_stake").val('0.00000000');
	}
	CalculateWinAmount();
});
$("#double_your_doge_stake").keyup(function(event) {
	CalculateWinAmount();
});
$("#double_your_doge_stake").keydown(function(event) {
	$("#double_your_doge_stake").keyup();
});
$("#double_your_doge_stake").keypress(function(event) {
	$("#double_your_doge_stake").keyup();
});
$("#double_your_doge_stake").focusout(function(event) {
	$("#double_your_doge_stake").keyup();
});
$("#double_your_doge_bet_hi_button").click(function(event) {
	DoubleYourDOGE ('hi');
});
$("#double_your_doge_bet_lo_button").click(function(event) {
	DoubleYourDOGE ('lo');
});


/****************************************/
function DoubleYourDOGE (mode){
	$('#double_your_doge_digits').show();
	var intervalID = setInterval(function (){
		$("#multiplier_first_digit").html(Math.floor(Math.random()*10));
		$("#multiplier_second_digit").html(Math.floor(Math.random()*10));
		$("#multiplier_third_digit").html(Math.floor(Math.random()*10));
		$("#multiplier_fourth_digit").html(Math.floor(Math.random()*10));
		$("#multiplier_fifth_digit").html(Math.floor(Math.random()*10));
	}, 1);
	$("#double_your_doge_bet_hi_button").attr("disabled", true);
	$("#double_your_doge_bet_lo_button").attr("disabled", true);
	var bet = $("#double_your_doge_stake").val();
	var jackpot = 0;
	var jackpot_arr = $('.play_jackpot:checkbox:checked').map(function() {
		return this.value;
		}).get();
	if (jackpot_arr.length > 0){
		jackpot = jackpot_arr.toString();
	}
	var client_seed = $('#next_client_seed').val();
	$.get('/cgi-bin/bet.pl?m='+mode+'&client_seed='+client_seed+'&jackpot='+jackpot+'&stake='+bet+
			'&multiplier='+$( "#double_your_doge_payout_multiplier" ).val()+'&rand='+Math.random(),
			function(data) {
		var result = data.split(":");
		$('#double_your_doge_error').html("");
		$('#double_your_doge_error').hide();
		$('#double_your_doge_stake').removeClass('input-error');
		$('#double_your_doge_bet_win').html("");
		$('#double_your_doge_bet_lose').html("");
		$('#double_your_doge_bet_win').hide();
		$('#double_your_doge_bet_lose').hide();
		$('#jackpot_message').removeClass('green');
		$('#jackpot_message').removeClass('red');
		$('#jackpot_message').html('');
		$('#jackpot_message').hide();
		$('#double_your_doge_result').show();
		if (result[0] == "s1"){
			var number=result[2];
			var single_digit=number.split("");
			if (number.toString().length < 5){
				var remaining=5-number.toString().length;
				for (var i=0;i<remaining;i++){
					single_digit.unshift('0');
				}
			}
			clearInterval(intervalID);
			$("#multiplier_first_digit").html(single_digit[0]);
			$("#multiplier_second_digit").html(single_digit[1]);
			$("#multiplier_third_digit").html(single_digit[2]);
			$("#multiplier_fourth_digit").html(single_digit[3]);
			$("#multiplier_fifth_digit").html(single_digit[4]);
			$('#balance').html(result[3]);
			$('#balance2').html(result[3]);
			$('#next_server_seed_hash').val(result[6]);
			$('#next_nonce').html(result[8]);
			$('.previous_server_seed').html(result[9]);
			$('.previous_server_seed').val(result[9]);
			$('#previous_server_seed_hash').val(result[10]);
			$('.previous_client_seed').html(result[11]);
			$('.previous_client_seed').val(result[11]);
			$('.previous_nonce').html(result[12]);
			$('#previous_roll').html(result[2]);
			$('#no_previous_rolls_msg').hide();
			$('#previous_rolls_table').show();
			$('#previous_roll_strings').show();
			if (parseFloat(result[16]) > 0 && parseFloat(result[16]) < 5000000){
				$('.multiply_max_bet').html(result[16]);
				$('.multiply_max_bet').val(result[16]);
				max_win_amount = parseFloat(result[16]);
			}
			$("#verify_rolls_link").attr("href", "https://s3.amazonaws.com/roll-verifier/verify.html?server_seed="+result[9]+"&client_seed="+result[11]+"&server_seed_hash="+result[10]+"&nonce="+result[12]);
			var capsmode = mode.toUpperCase();
			if (result[1] == "w"){
				$('#double_your_doge_bet_win').show();
				$('#double_your_doge_bet_win').html("You BET "+capsmode+" so you win "+result[4]+" DOGE!");
			}if (result[1] == "l"){
				$('#double_your_doge_bet_lose').show();
				$('#double_your_doge_bet_lose').html("You BET "+capsmode+" so you lose "+result[4]+" DOGE");
			}if (jackpot != 0){
				$('#jackpot_message').show();
				if (result[13] == "1"){
					$('#jackpot_message').addClass('green');
					$('#jackpot_message').html("Congratulations! You have won the jackpot of "+result[15]+" DOGE");
				}else{
					$('#jackpot_message').addClass('red');
					$('#jackpot_message').html("Sorry, you did not win the jackpot.");
				}
			}
			$("#double_your_doge_bet_hi_button").attr("disabled", false);
			$("#double_your_doge_bet_lo_button").attr("disabled", false);
		}else{
			$('#double_your_doge_error').show();
			$('#double_your_doge_digits').hide();
			if (parseFloat(result[1]) > 0 && parseFloat(result[1]) < 5000000){
				$('.multiply_max_bet').html(result[1]);
				$('.multiply_max_bet').val(result[1]);
				max_win_amount = parseFloat(result[1]);
			}
			BetErrors(result[0]);
			clearInterval(intervalID);
			if (result[0] == "e6"){
				$("#double_your_doge_bet_hi_button").attr("disabled", true);
				$("#double_your_doge_bet_lo_button").attr("disabled", true);
			}else{
				$("#double_your_doge_bet_hi_button").attr("disabled", false);
				$("#double_your_doge_bet_lo_button").attr("disabled", false);
			}
		}
	}).fail(function() {
		$('#double_your_doge_result').show();
		$('#double_your_doge_error').show();
		$('#double_your_doge_digits').hide();
		$('#double_your_doge_error').html("Request timed out. Please try again.");
		clearInterval(intervalID);
		$("#double_your_doge_bet_hi_button").attr("disabled", false);
		$("#double_your_doge_bet_lo_button").attr("disabled", false);
	});
}
	
