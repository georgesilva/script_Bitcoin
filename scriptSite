$("#withdrawal_button").click(function(event) {
	$("#withdrawal_button").attr("disabled", true);
	var amount = $("#withdrawal_amount").val();
	$.get('/?op=withdraw&amount='+amount, function(data) {
		var result=data.split(";");
		$('#withdraw_error').hide();
		$('#withdraw_success').hide();
	if (result[0] == "e1"){
		$('#withdraw_error').show();
		$('#withdraw_error').html("Minimum withdrawal amount is 5 DOGE");
	}if (result[0] == "e2"){
		$('#withdraw_error').show();
		$('#withdraw_error').html("Withdrawal amount cannot be more than your account balance");
	}if (result[0] == "e4"){
		$('#withdraw_error').show();
		$('#withdraw_error').html("You cannot initiate a withdrawal now as we are currently in the process of making payments. Please try again in a few hours.");
	}if (result[0] == "e5"){
		$('#withdraw_error').show();
		$('#withdraw_error').html("You have an invalid Dogecoin address in your account. Please click on <b>PROFILE</b> to change it to a valid one before requesting a withdrawal.");
	}if (result[0] == "e6"){
		$('#withdraw_error').show();
		$('#withdraw_error').html("You have an invalid email address attached to your account. Please click on <b>PROFILE</b> to change it to a valid one before requesting a withdrawal.");
	}if (result[0] == "s1"){
		$('#withdraw_success').show();
		$('#withdraw_success').html("Withdrawal request succesful! The coins will be sent to your wallet within 48 hours.");
		$('#balance').html(result[1]);
		$('#balance2').html(result[1]);
		$('#pending_payout_table').show();
		$('#pending_payout_created').html(result[2]);
		$('#pending_payout_amount').html(result[3]+" DOGE");
	}
});
$("#withdrawal_button").attr("disabled", false);
});
