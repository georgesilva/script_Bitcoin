$("#double_your_doge_tab").on('keydown', '#double_your_doge_stake', function(e){
	var keyCode = e.keyCode || e.which;
	if (keyCode == 72) {
		e.preventDefault();
		if ($("#double_your_doge_bet_hi_button").is(":enabled")){
			$("#double_your_doge_bet_hi_button").click();
		}
	}else if (keyCode == 76) {
		e.preventDefault();
		if ($("#double_your_doge_bet_lo_button").is(":enabled")){
			$("#double_your_doge_bet_lo_button").click();
		}
	}else if (keyCode == 65) {
		e.preventDefault();
		if ($("#double_your_doge_stake").val() > 0.00000001){
			$("#double_your_doge_half").click();
		}
	}else if (keyCode == 83) {
		e.preventDefault();
		$("#double_your_doge_2x").click();
	}else if (keyCode == 68) {
		e.preventDefault();
		$("#double_your_doge_min").click();
	}else if (keyCode == 70) {
		e.preventDefault();
		$("#double_your_doge_max").click();
	}else if (keyCode == 81) {
		e.preventDefault();
		$("#double_your_doge_payout_multiplier").val(parseFloat($("#double_your_doge_payout_multiplier").val())+1);
		$("#double_your_doge_payout_multiplier").keyup();
	}else if (keyCode == 87) {
		e.preventDefault();
		$("#double_your_doge_payout_multiplier").val(parseFloat($("#double_your_doge_payout_multiplier").val())-1);
		$("#double_your_doge_payout_multiplier").keyup();
	}else if (keyCode == 69) {
		e.preventDefault();
		$("#double_your_doge_payout_multiplier").val(parseFloat($("#double_your_doge_payout_multiplier").val())+5);
		$("#double_your_doge_payout_multiplier").keyup();
	}else if (keyCode == 82) {
		e.preventDefault();
		$("#double_your_doge_payout_multiplier").val(parseFloat($("#double_your_doge_payout_multiplier").val())-5);
		$("#double_your_doge_payout_multiplier").keyup();
		}else if (keyCode == 84) {
			e.preventDefault();
			$("#double_your_doge_payout_multiplier").val(parseFloat($("#double_your_doge_payout_multiplier").val())+10);
			$("#double_your_doge_payout_multiplier").keyup();
		}else if (keyCode == 89) {
			e.preventDefault();
			$("#double_your_doge_payout_multiplier").val(parseFloat($("#double_your_doge_payout_multiplier").val())-10);
			$("#double_your_doge_payout_multiplier").keyup();
		}
	});
